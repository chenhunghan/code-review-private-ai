{
  "name": "code-review-private-ai",
  "version": "0.0.26",
  "description": "Your private AI code reviewer",
  "bin": {
    "code-review-private-ai": "./dist/index.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "tsx ./src/index.ts review",
    "start-ci": "tsx ./src/index.ts review --ci",
    "build": "node utils/build.js",
    "postbuild": "node utils/shebang.js && chmod +x ./dist/index.js"
  },
  "keywords": [
    "code-review",
    "review",
    "vertexai",
    "huggingface",
    "ai",
    "genai",
    "llama"
  ],
  "author": "Hung-Han Chen",
  "license": "MIT",
  "dependencies": {
    "@actions/github": "^5.1.1",
    "inquirer": "^9.2.8",
    "langchain": "^0.0.103",
    "ts-retry": "^4.2.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.3",
    "@types/node": "^20.4.0",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "esbuild": "^0.18.11",
    "eslint": "^8.44.0",
    "eslint-config-standard-with-typescript": "^36.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "npm-dts": "^1.3.12",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6"
  },
  "files": [
    "dist/*"
  ]
}
